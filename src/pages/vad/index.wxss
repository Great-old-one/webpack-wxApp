
Page {
  width: 100%;
  height: 100%;
}

.container {
  width: 100%;
  height: 100%;
  padding-bottom: 100px;
}

.section {
  background-color: #fff;
  margin-top: 20rpx;
}

.section .title {
  height: 90rpx;
  line-height: 90rpx;
  margin-left: 28rpx;
  font-size: 28rpx;
  font-weight: bold;
  border-bottom: 1px solid #E4E4E4;
}

.section .items {
  padding: 15rpx 30rpx;
}

.section .items .item {
  height: 60rpx;
  line-height: 60rpx;
}

.section .items .item .item-title {
  display: inline-block;
  width: 150rpx;
  color: #666;
  font-size: 30rpx;
}

.section .items .item .item-content {
  display: inline-block;
  color: #333;
  font-size: 30rpx;
}

.footer {
  position: fixed;
  height: 100rpx;
  bottom: 0;
  width: 100%;
  background-color: #fff;
}

.home {
  display: inline-block;
  width: 25%;
  float: left;
}

.home image {
  display: table;
  margin: 14rpx auto 0 auto;
  width: 36rpx;
  height: 40rpx;
}

.home text {
  font-size: 20rpx;
  color: #666666;
  display: block;
  text-align: center;
  margin-top: 10rpx;
}

.contact {
  display: inline-block;
  width: 75%;
  text-align: center;
  background-color: #F03D37;
}

.contact-wrapper {
  display: table;
  height: 40rpx;
  line-height: 40rpx;
  margin: 30rpx auto;
}

.contact-wrapper image{
  height:40rpx;
  width:40rpx;
  float: left;
  margin-right: 14rpx;
}

.contact-wrapper text {
  color: #fff;
}



